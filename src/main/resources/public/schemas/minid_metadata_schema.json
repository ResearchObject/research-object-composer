{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "length": {
      "type": "integer"
    },
    "filename": {
      "type": "string"
    },
    "md5": {
      "type": "string",
      "pattern": "^[0-9a-f]{32}$"
    },
    "sha256": {
      "type": "string",
      "pattern": "^[0-9a-f]{64}$"
    },
    "sha512": {
      "type": "string",
      "pattern": "^[0-9a-f]{128}$"
    },
    "url": {
      "type": "string",
      "format": "uri"
    }
  },
  "required" : ["length", "filename", "url", "sha512"]
}
